<Window x:Class="CvOnline.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CvOnline"
        mc:Ignorable="d"
        Title="Login" Height="300" Width="300">
    <Grid>
        <Label x:Name="label" Content="LOGIN" HorizontalAlignment="Left" Margin="119,51,0,0" VerticalAlignment="Top" Height="31" Width="53"/>
        <Label x:Name="label1" Content="Email" HorizontalAlignment="Left" Margin="37,94,0,0" VerticalAlignment="Top" Width="73" Height="25"/>
        <TextBox x:Name="textBoxEmail" HorizontalAlignment="Left" Height="25" Margin="119,94,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150"/>
        <Label x:Name="label2" Content="Password" HorizontalAlignment="Left" Margin="37,135,0,0" VerticalAlignment="Top"/>
        <Button x:Name="buttonLogin" Content="LOGIN" HorizontalAlignment="Left" Margin="108,188,0,0" VerticalAlignment="Top" Width="75" Click="buttonLogin_Click"/>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Left" Margin="119,135,0,0" VerticalAlignment="Top" Width="150" Height="27"/>
    </Grid>
</Window>
